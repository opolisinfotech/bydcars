<!DOCTYPE html>
<html>
<head>
    <%- include('../elements/header'); %>
</head>
<body class="hold-transition sidebar-mini layout-fixed">
<div class="wrapper">
  <%- include('../elements/navigation'); %>
  <!-- Main Sidebar Container -->
  <aside class="main-sidebar sidebar-dark-primary elevation-4">
    <%- include('../elements/sidebar'); %>
  </aside>
  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-left">
              <li class="breadcrumb-item"><a href="<%=nodeAdminUrl%>/Dashboard">Dashboard</a></li>
              <li class="breadcrumb-item active">SME Contact</li>
            </ol>
          </div>
        </div>
      </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-12">
          <div class="card">
            <div class="card-header">
			  <div class="row mb-2">
				<div class="col-sm-6"><h3 class="card-title">SME Contact</h3></div>
				<div class="col-sm-6"><a href="<%=nodeAdminUrl%>/download/smecontact" class="float-right">Export</a></div>
			  </div>
            </div>
            <!-- /.card-header -->
            <div class="card-body">
              <table id="example1" class="table table-bordered table-striped">
                <% if (data.length > 0) {%>
				<thead>
                <tr>
                  <th>ID</th>
				  <th>Email</th>
				  <th>Message Date</th>
				  <th>Message</th>
                  <!-- <th class="rsicon">Action</th> -->
                </tr>
                </thead>
                <tbody>
				<% data.forEach((datarow, index) => { %>
                <tr>
                  <td><%= datarow.sn %></td>
				  
				  <td><%= datarow.emailid %></td>
				  <td><%= datarow.message_date %></td>
				  <td><%= datarow.message %></td>
                <!-- <td><a href="/admin/testdrive/edit?id=<%= datarow.id %>">Edit</a> | <a href="/admin/testdrive/delete?id=<%= datarow.id %>" onClick="return confirm('Are you sure you want to delete ?')">Delete</a></td>  --> 
                </tr>
				<% }) %>
                </tbody>
                <!--<tfoot>
                <tr>
                  <th>ID</th>
                  <th>Name</th>
                  <th>Action</th>
                </tr>
                </tfoot>-->
				<% } else { %>
				 <tr>
				 <td class="text-center">No data found.</td>
				 </tr>
                <% } %>
              </table>
            </div>
            <!-- /.card-body -->
          </div>
          <!-- /.card -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
    <!-- /.content -->
  </div>
  <%- include('../elements/footer'); %>
</div>
<!-- ./wrapper -->
<%- include('../elements/js'); %>
<!-- page script -->
<script>
  $(function () {
    $("#example1").DataTable();
    $('#example2').DataTable({
      "paging": true,
      "lengthChange": false,
      "searching": false,
      "ordering": true,
      "info": true,
      "autoWidth": false,
    });
  });
</script>
</body>
</html>
